@model IEnumerable<YourProjectNamespace.Models.Db.User>

@{
    ViewData["Title"] = "Список пользователей";
}

<h2>Список пользователей</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Идентификатор</th>
            <th>Имя</th>
            <th>Фамилия</th>
            <th>Дата регистрации</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.JoinDate</td>
            </tr>
        }
    </tbody>
</table>
